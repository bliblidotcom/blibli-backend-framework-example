root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 2

[*.java]
indent_size = 2
tab_width = 2

[{*.jhm, *.xjb, *.rng, *.wsdl, *.wsdd, *.fxml, *.pom, *.xslt, *.jrxml, *.ant, *.xul, *.xsl, *.xsd, *.tld, *.jnlp, *.wadl, *.xml}]
indent_size = 2
tab_width = 2

[{*.zsh, *.bash, *.sh}]
indent_size = 2
tab_width = 2

[{.babelrc, .eslintrc, jest.config, bowerrc, .stylelintrc, *.jsb3, *.jsb2, *.json}]
indent_size = 2
